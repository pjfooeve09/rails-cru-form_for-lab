<%= form_for(@song) do |f| %>
    <p>
        <%= f.label :name %><br/>
        <%= f.text_field :name %>
    </p>
    <p>
        <%= f.label :artist %>
        <%= f.text_field :artist_id %>
    </p>
    <p>
        <%= f.label :genre %>
        <%= f.text_field :genre_id %>
    </p>
    <%= f.submit "Create Song" %>
<% end %>